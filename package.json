    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "localforage": "^1.3.1",
    "package-json-versionify": "^1.0.1",
    "regenerator": "^0.8.42",
    "semver": "^5.1.0",
    "serviceworker-cache-polyfill": "^3.0.0"
